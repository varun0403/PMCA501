#include<iostream>
#include<malloc.h>
using namespace std;

struct node{
	int emp_id;
	struct node* prev;
	struct node* next;
};

struct node* head = NULL;

void traverse(){
    struct node* temp = head;
    while(temp->next != NULL){
        cout<<temp->emp_id<<"->";
        temp = temp->next;
    }
    cout<<temp->emp_id<<endl;
}

void insert(int data){
	struct node* new_node = (struct node*) malloc(sizeof(struct node));
	new_node->emp_id = data;
	new_node->next = NULL;
	new_node->prev = NULL;
	cout<<"The inserted node is "<<new_node->emp_id<<endl;
	if(head == NULL){
		head = new_node;
	}
	else{
		new_node->next = head;
		head->prev = new_node;
		head = new_node;
	}
    traverse();
}

int main(){
    int n,c;
    cout<<"Enter limit: ";
    cin>>n;
    for(int i=0; i<n; i++){
        cout<<"\nEnter emp id: ";
        cin>>c;
        insert(c);
    }
    return 0;
}
